<h1> Your peeps </h1>

<ul id='peeps'>
  <% @peeps.each do |peep| %>
  <li>
    Peep: <%= peep.message %>
    <br>
    Peeped at: <%= peep.created_at.strftime("%F %T") %>
  </li>
  <br></br>
  <% end %>

</ul>

<form action="/peeps/new" method="get">
  <input type="submit" value="Write a Peep">
</form>
